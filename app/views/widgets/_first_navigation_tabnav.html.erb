    end
  end

=begin
  add_tab do |t|
    t.named I18n.t(:about)
    t.links_to_remote :url => about_url
  end
  add_tab do |t|
    t.named I18n.t(:contact)
    t.links_to_remote :url => contact_url
  end
=end
end 
%>
