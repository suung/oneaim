<h1>Editing project</h1>

<% remote_form_for(@project) do |f| %>
  <%= f.error_messages %>


  <p>
    <%= f.label :icon %></br>
    <%= f.text_field :icon %> </br>
  </p>

  <p>
   <%= f.label :small_icon %><br/>
   <%= f.text_field :small_icon %><br/>
  </p>


  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.label :is_open %><br />
    <%= f.check_box :is_open %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= show_link @project %> |
<%= index_link :pages %>
