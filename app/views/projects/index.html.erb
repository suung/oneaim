<h1>Listing projects</h1>

<table>
  <tr>
    <th>Icon</th>
    <th>Title</th>
    <th>Description</th>
    <th>Url</th>
    <th>Is open</th>
  </tr>

<% @projects.each do |project| %>
  <tr>
    <td><%= image_tag(project.icon) %></td>
    <td><%=h project.title %></td>
    <td><%=h project.description %></td>
    <td><%=h project.url %></td>
    <td><%=h project.is_open %></td>
    <td><%= show_link project %></td>
    <td><%= edit_link project %></td>
    <td><%= delete_link project %></td>
  </tr>
<% end %>
</table>

<br />

<%= new_link :project %>
