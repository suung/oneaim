<style>
.<%=@name%>_tabnav {
  background-color: #FFFFFF;
  padding: 0;
  height: 2em;
  border-bottom: 0.35em solid #6699CC;
}

.<%=@name%>_tabnav ul {
  height: 2em;
  margin: 0;
  padding: 0 1.25em;
}

.<%=@name%>_tabnav li {
  display: inline;
  line-height: 1.167em;
  padding: 0;
}

.<%=@name%>_tabnav li.secondary {
  float: right;
}

.<%=@name%>_tabnav li a {
  background:#CFDFFD url(../../images/widgets/menu-bg.png) repeat-x scroll;
  color: #626262;
  text-decoration: none;
  border: 0.083em solid #FFFFFF;
  border-bottom: 0.083em solid #CFDFFD;
  padding: 0.25em 0.667em 0.417em;
  float: left;
}

.<%=@name%>_tabnav li a.active {
  color: #FFFFFF;
  font-weight: bold;
  background:#6699CC url(../../images/widgets/menu-bg.png) repeat-x scroll;
  border: 0.083em solid #FFFFFF;
  border-bottom: 0.083em solid #6699CC;
}

.<%=@name%>_tabnav li a:hover {
  color: #90A8D8;
}

.<%=@name%>_tabnav li a.active:hover {
  color: #90A8D8;
}

.<%=@name%>_tabnav_content {
  background-color: #FFFFFF;
  padding: 1.5em 0.417em;
  margin-top: 3px;
  border: 1px solid #6699CC;
}
</style>
